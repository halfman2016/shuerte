                android:id="@+id/btnNum16"
                android:layout_width="10mm"
                android:layout_height="match_parent"
                android:text="1"
                android:textAlignment="center"
                android:textSize="30sp" />

            <Button
                android:id="@+id/btnNum17"
                android:layout_width="10mm"
                android:layout_height="match_parent"
                android:text="2"
